-- Scripts:

local function ZLLLOG_fake_script() -- Goto.goto script 
	local script = Instance.new('Script', Goto)

	local button = script.Parent
	button.MouseButton1Click:Connect(function()
		local LocalPlayer = game:GetService("Players").LocalPlayer
		local LocalCharacter = LocalPlayer.Character
		local lastpos = LocalCharacter.HumanoidRootPart.CFrame task.wait()
		LocalCharacter.HumanoidRootPart.CFrame = game.Players[target].Character.LowerTorso.CFrame
	end)
end
coroutine.wrap(ZLLLOG_fake_script)()
local function KBQPA_fake_script() -- Frame.currency update 
	local script = Instance.new('Script', Frame)

	
	function comma_value(n) -- credit http://richard.warburton.it
		local left,num,right = string.match(n,'^([^%d]*%d)(%d*)(.-)$')
		return left..(num:reverse():gsub('(%d%d%d)','%1,'):reverse())..right
	end
	
	while true do wait()
		pcall(function()
			local money = game:GetService("Players")[target].DataFolder.Currency.Value
			currency.Text = (comma_value('$'..money))
		end)
	end
	
	
end
coroutine.wrap(KBQPA_fake_script)()
local function UYDTNJ_fake_script() -- Reset.Reset 
	local script = Instance.new('Script', Reset)

	local button = script.Parent
	button.MouseButton1Click:Connect(function()
	
			for _, x in pairs(game:GetService('Players').LocalPlayer.Character:GetChildren()) do
				if x:IsA("BasePart") or x:IsA('Accessory') then
					x:Destroy()
				end
			end
	
	end)
	
end
coroutine.wrap(UYDTNJ_fake_script)()
local function DLOXFP_fake_script() -- view.view 
	local script = Instance.new('Script', view)

	local button = script.Parent
	button.MouseButton1Click:Connect(function()
	
		
		if view.Text == 'View' then
			view.Text = 'Un-view'
			repeat wait() 
				pcall(function()
					game.Workspace.Camera.CameraSubject = game.Players[target].Character.Humanoid
				end)
			until view.Text == 'View'
			
			game.Workspace.Camera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
		else
			view.Text = 'View'
			
		end
		
			
	end)
		
end
coroutine.wrap(DLOXFP_fake_script)()
local function EQRRQ_fake_script() -- LastPos.Lastpos script 
	local script = Instance.new('Script', LastPos)

	local button = script.Parent
	button.MouseButton1Click:Connect(function()
		local LocalPlayer = game:GetService("Players").LocalPlayer
		local LocalCharacter = LocalPlayer.Character
		LocalCharacter.HumanoidRootPart.CFrame = lastpos
	end)
end
coroutine.wrap(EQRRQ_fake_script)()
local function REMOGMT_fake_script() -- targetgui.name shrink 
	local script = Instance.new('Script', targetgui)

	local function ShrinkName()
		player.FocusLost:connect(function()
			for i,v in pairs(game.Players:GetPlayers()) do
				if v.Name:lower():match("^"..player.Text:lower()) or v.DisplayName:lower():match("^"..player.Text:lower()) then
					target = v.Name
					targetdisp = v.DisplayName
					player.Text = target..' / '..targetdisp
					break
				end
			end
		end)
	end
	
	ShrinkName()
	
end
coroutine.wrap(REMOGMT_fake_script)()
local function SXVICI_fake_script() -- targetgui.toggle gui 
	local script = Instance.new('Script', targetgui)

	local ui = script.Parent.Frame
	game:GetService("UserInputService").InputBegan:Connect(function(key, gp)
		if key.KeyCode == Enum.KeyCode.Insert then
			if ui.Visible == true then
				ui.Visible = false
			elseif ui.Visible == false then
				ui.Visible = true
			end
		end
	end)
end
coroutine.wrap(SXVICI_fake_script)()
local function BNEMVC_fake_script() -- targetgui.anticheat 
	local script = Instance.new('Script', targetgui)

	assert(getrawmetatable)
	gmt = getrawmetatable(game)
	setreadonly(gmt, false)
	old = gmt.__namecall
	gmt.__namecall =
		newcclosure(
			function(self, ...)
				local args = {...}
				if tostring(args[1]) == "BreathingHAMON" then
				return
			elseif tostring(args[1]) == "TeleportDetect" then
				return
			elseif tostring(args[1]) == "CHECKER_1" then
				return
			elseif tostring(args[1]) == "CHECKER" then
				return
			elseif tostring(args[1]) == "GUI_CHECK" then
				return
			elseif tostring(args[1]) == "OneMoreTime" then
				return
			elseif tostring(args[1]) == "checkingSPEED" then
				return
			elseif tostring(args[1]) == "BANREMOTE" then
				return
			elseif tostring(args[1]) == "PERMAIDBAN" then
				return
			elseif tostring(args[1]) == "KICKREMOTE" then
				return
			elseif tostring(args[1]) == "BR_KICKPC" then
				return end
	
				return old(self, ...)
			end)
	
end
coroutine.wrap(BNEMVC_fake_script)()
local function KDOE_fake_script() -- targetgui.drag 
	local script = Instance.new('Script', targetgui)

	script.Parent.Frame.Draggable = true
end
coroutine.wrap(KDOE_fake_script)()
